<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="selectEmpById">
        SELECT
            EMP_ID,
            EMP_NAME,
            DEPT_CODE,
            JOB_CODE,
            SALARY
          FROM EMPLOYEE
         WHERE EMP_ID = ?
    </entry>
</properties>